<html>
<head><title>odd</title>
<script type="text/javascript">
function odd(){
var a= Number(document.getElementById("N1").value);
var b= Number(document.getElementById("N2").value);
var c;
var s;
for(c=a;c<=b;c++)
{
  s=c%2;
  if(s!=0)
	{
		document.write(c);
		document.write("<br>");
	}
}
}
</script>
</head>
<body>
	Enter first number: <input id="N1"><br>
	Enter second number: <input id="N2"><br>
	<button onClick="odd()">Get odd numbers between them</button><br>
</body>
</html>
